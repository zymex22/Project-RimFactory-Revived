<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <WorkGiverDef>
    <defName>DoBillsRoboticMachiningTable</defName>
    <label>do bills at robotic machining table</label>
    <giverClass>WorkGiver_DoBill</giverClass>
    <workType>Smithing</workType>
    <priorityInType>75</priorityInType>
    <fixedBillGiverDefs>
      <li>TableRoboticMachining</li>
    </fixedBillGiverDefs>
    <verb>work</verb>
    <gerund>working at</gerund>
    <requiredCapacities>
      <li>Manipulation</li>
    </requiredCapacities>
    <prioritizeSustains>true</prioritizeSustains>
  </WorkGiverDef>
  
  <WorkGiverDef>
    <defName>DoBillsPRF_MineShaft</defName>
    <label>do bills at mine shaft</label>
    <giverClass>WorkGiver_DoBill</giverClass>
    <workType>Mining</workType>
    <priorityInType>75</priorityInType>
    <fixedBillGiverDefs>
      <li>PRF_MineShaft</li>
    </fixedBillGiverDefs>
    <verb>mine</verb>
    <gerund>mining at</gerund>
    <requiredCapacities>
      <li>Manipulation</li>
    </requiredCapacities>
    <prioritizeSustains>true</prioritizeSustains>
  </WorkGiverDef>
  
</Defs>