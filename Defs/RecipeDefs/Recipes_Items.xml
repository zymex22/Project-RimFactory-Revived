<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--==================================== DataDisk ==============================-->
  <RecipeDef>
    <defName>MakeDataDisk</defName>
    <label>make data disk</label>
    <description>Make a roll of magnetic data tape.</description>
    <jobString>Making data disk.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>5000</workAmount>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>50</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentIndustrial</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
        <li>ComponentIndustrial</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <PRF_DataDisk>1</PRF_DataDisk>
    </products>
    <skillRequirements>
      <Crafting>8</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
    <recipeUsers>
      <li>TableMachining</li>
      <li>TableRoboticMachining</li>
    </recipeUsers>
    <researchPrerequisite>PRF_MagneticTape</researchPrerequisite>
  </RecipeDef>

