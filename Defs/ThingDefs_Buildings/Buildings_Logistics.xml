    </building>
    <modExtensions>
          <li Class="ProjectRimFactory.AutoMachineTool.ModExtension_Puller">
        <outputSides>false</outputSides>
      </li>
      <li Class="ProjectRimFactory.Common.ModExtension_Graphic">
