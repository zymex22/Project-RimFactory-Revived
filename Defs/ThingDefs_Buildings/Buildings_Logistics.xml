            <graphicData>
              <texPath>Belts/GrowzoneWorking</texPath>
              <graphicClass>Graphic_Multi</graphicClass>
              <drawSize>(1.1,1.1)</drawSize>
            </graphicData>
          </Working>
        </graphicDataList>
      </li>
    </modExtensions>
    <researchPrerequisites Inherit="false">
      <li>PRF_BeltsII</li>
    </researchPrerequisites>
  </ThingDef>

