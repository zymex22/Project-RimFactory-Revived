  <!--======================================= Growzone Puller ===============================================-->
  <ThingDef ParentName="PRF_Puller_Base">
    <defName>PRF_GrowzonePuller_I</defName>
    <thingClass>ProjectRimFactory.AutoMachineTool.Building_HarvestPuller</thingClass>
    <label>growzone puller</label>
    <description>A specialized puller that will pull from adjacent grow zones. It must be connected to the grow zone to function properly.</description>
	<disableImpassableShotOverConfigError>true</disableImpassableShotOverConfigError>
	<placeWorkers>
      <li>ProjectRimFactory.Common.PlaceWorker_GrowZonePuller</li>
    </placeWorkers>
    <costList>      <!-- Remove in Lite -->
      <PRF_RoboticArm>1</PRF_RoboticArm>
      <PRF_MachineFrame_I>1</PRF_MachineFrame_I>
    </costList>
    <statBases>
      <MarketValue>597</MarketValue>
    </statBases>
    <passability>Impassable</passability>
    <altitudeLayer>Building</altitudeLayer>
    <graphicData>
      <texPath>Belts/Growzone</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(1.1,1.1)</drawSize>
    </graphicData>
    <building>
      <fixedStorageSettings>
        <priority>Normal</priority>
        <filter>
          <categories>
            <li>Manufactured</li>
            <li>ResourcesRaw</li>
            <li>AnimalProductRaw</li>
            <li>Drugs</li>
            <li>EggsFertilized</li>
            <li>EggsUnfertilized</li>
            <li>Foods</li>
            <li>Medicine</li>
            <li>PlantMatter</li>
            <li>PlantFoodRaw</li>
          </categories>
          <specialFiltersToDisallow>
            <li>AllowRotten</li>            <!-- Disallow rotten by default -->
          </specialFiltersToDisallow>
        </filter>
      </fixedStorageSettings>
    </building>
    <modExtensions>
      <li Class="ProjectRimFactory.AutoMachineTool.ModExtension_Puller">
        <outputSides>false</outputSides>
      </li>
      <li Class="ProjectRimFactory.Common.ModExtension_Graphic">
        <graphicDataList>
          <Working>
            <graphicData>
              <texPath>Belts/GrowzoneWorking</texPath>
              <graphicClass>Graphic_Multi</graphicClass>
              <drawSize>(1.1,1.1)</drawSize>
            </graphicData>
          </Working>
        </graphicDataList>
      </li>
    </modExtensions>
    <researchPrerequisites Inherit="false">
      <li>PRF_BeltsII</li>
    </researchPrerequisites>
  </ThingDef>

