      <success>Always</success>
      <operations>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/ThingDef[defName = "PRF_SelfCookerII"]/recipes</xpath>
			<value>
				<li>VCE_CookChirashizushiSimple</li>
				<li>VCE_CookChirashizushiSimpleBulk</li>
				<li>VCE_CookNorimakiSimple</li>
				<li>VCE_CookNorimakiSimpleBulk</li>
				<li>VCE_CookUramakiFine</li>
				<li>VCE_CookUramakiFineBulk</li>
				<li>VCE_CookNigiriFine</li>
				<li>VCE_CookNigiriFineBulk</li>
          </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/ThingDef[defName = "PRF_SelfCookerIII"]/recipes</xpath>
			<value>
				<li>VCE_CookChirashizushiSimple</li>
				<li>VCE_CookChirashizushiSimpleBulk</li>
				<li>VCE_CookNorimakiSimple</li>
				<li>VCE_CookNorimakiSimpleBulk</li>
