<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<!--RimFridge -->
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>FishIndustry</li>
			<li>Fishing Nets</li>
			<li>[RF] Fishing</li>
			<li>Thanks For All The Fish</li>
			<li>Vanilla Fishing Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<!-- ============== Adding T1 Aquaculture =============== -->
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						  <ThingDef ParentName="PRF_IndustrialBase">
							<defName>PRF_T1_Aquaculture</defName>
							<label>Automatic Fish Farm (freshwater)</label>
							<description>An automated aquaculture system that will farm freshwater fish on request.</description>
							<thingClass>ProjectRimFactory.SAL3.Things.Assemblers.Building_SimpleAssembler</thingClass>
							<graphicData>
							  <texPath>Industry/aquaT1</texPath>
							  <drawSize>(5.5,5.5)</drawSize>
							</graphicData>
							<uiIconScale>0.8</uiIconScale>
							<designatorDropdown>AnimalStations_gr</designatorDropdown>
							<size>(5,5)</size>
							<inspectorTabs>
							  <li>ProjectRimFactory.SAL3.UI.ITab_SAL3Bills</li>
							</inspectorTabs>
							<statBases>
							  <WorkToBuild>2500</WorkToBuild>
							  <MaxHitPoints>250</MaxHitPoints>
							  <MarketValue>6522</MarketValue>
							  <Flammability>1.0</Flammability>
							</statBases>
							<costList>
							  <Steel>150</Steel>
							  <Plasteel>200</Plasteel>
							  <ComponentIndustrial>10</ComponentIndustrial>
							  <ComponentSpacer>2</ComponentSpacer>
							  <PRFMachineFrameLarge>1</PRFMachineFrameLarge>
							  <PRFRoboticArm>2</PRFRoboticArm>
							  <PRFAdvancedAIChip>1</PRFAdvancedAIChip>
							</costList>
							<recipes>
							  <li>PRF_FreshwaterFish</li>
							</recipes>
							<comps>
							  <li Class="CompProperties_Power">
								<compClass>CompPowerTrader</compClass>
								<basePowerConsumption>600</basePowerConsumption>
							  </li>
							  <li Class="CompProperties_Breakdownable"/>
							  <li>
								<compClass>ProjectRimFactory.Common.CompOutputAdjustable</compClass>
							  </li>
							  <li Class="CompProperties_Flickable" />
							  <li Class="CompProperties_Glower">
								<glowRadius>4</glowRadius>
								<glowColor>(115,198,206,0)</glowColor>
							  </li>
							</comps>
							<modExtensions>
							  <li Class="ProjectRimFactory.SAL3.Exposables.AssemblerDefModExtension">
								<drawStatus>true</drawStatus>
								<doEffect>false</doEffect>
								<workSpeedBaseFactor>1.0</workSpeedBaseFactor>
							  </li>
							  <li Class="ProjectRimFactory.Common.ModExtension_Skills">
								<SkillUsage>ThisOverrides</SkillUsage>
								<BaseSkill>10</BaseSkill>
								<skills>
								  <li>
									<def>Cooking</def>
									<levelInt>15</levelInt>
								  </li>
								</skills>
							  </li>
							  <li Class="ProjectRimFactory.Common.ModExtension_BonusYield">
								<bonusYields Chance="1">
								</bonusYields>
							  </li>
							</modExtensions>
							<staticSunShadowHeight>1</staticSunShadowHeight>
							<researchPrerequisites>
							  <li>PRF_AnimalStationsII</li>
							</researchPrerequisites>
						  </ThingDef>
					</value>
				</li>
				<!-- ============== Adding t2 aquaculture =============== -->
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						  <ThingDef ParentName="PRF_IndustrialBase">
							<defName>PRF_T2_Aquaculture</defName>
							<label>Automatic Fish Farm (saltwater)</label>
							<description>An automated aquaculture system that will farm saltwater fish on request.</description>
							<thingClass>ProjectRimFactory.SAL3.Things.Assemblers.Building_SimpleAssembler</thingClass>
							<graphicData>
							  <texPath>Industry/aquaT2</texPath>
							  <drawSize>(5.5,5.5)</drawSize>
							</graphicData>
							<uiIconScale>0.8</uiIconScale>
							<designatorDropdown>AnimalStations_gr</designatorDropdown>
							<size>(5,5)</size>
							<inspectorTabs>
							  <li>ProjectRimFactory.SAL3.UI.ITab_SAL3Bills</li>
							</inspectorTabs>
							<statBases>
							  <WorkToBuild>3000</WorkToBuild>
							  <MaxHitPoints>250</MaxHitPoints>
							  <MarketValue>7405</MarketValue>
							  <Flammability>1.0</Flammability>
							</statBases>
							<costList>
							  <Steel>175</Steel>
							  <Plasteel>225</Plasteel>
							  <ComponentIndustrial>15</ComponentIndustrial>
							  <ComponentSpacer>6</ComponentSpacer>
							  <PRFMachineFrameLarge>1</PRFMachineFrameLarge>
							  <PRFRoboticArm>2</PRFRoboticArm>
							  <PRFAdvancedAIChip>1</PRFAdvancedAIChip>
							</costList>
							<recipes>
							  <li>PRF_OceanwaterFish</li>
							</recipes>
							<comps>
							  <li Class="CompProperties_Power">
								<compClass>CompPowerTrader</compClass>
								<basePowerConsumption>650</basePowerConsumption>
							  </li>
							  <li Class="CompProperties_Breakdownable"/>
							  <li>
								<compClass>ProjectRimFactory.Common.CompOutputAdjustable</compClass>
							  </li>
							  <li Class="CompProperties_Flickable" />
							  <li Class="CompProperties_Glower">
								<glowRadius>4</glowRadius>
								<glowColor>(115,198,206,0)</glowColor>
							  </li>
							</comps>
							<modExtensions>
							  <li Class="ProjectRimFactory.SAL3.Exposables.AssemblerDefModExtension">
								<drawStatus>true</drawStatus>
								<doEffect>false</doEffect>
								<workSpeedBaseFactor>1.5</workSpeedBaseFactor>
							  </li>
							  <li Class="ProjectRimFactory.Common.ModExtension_Skills">
								<SkillUsage>ThisOverrides</SkillUsage>
								<BaseSkill>10</BaseSkill>
								<skills>
								  <li>
									<def>Cooking</def>
									<levelInt>15</levelInt>
								  </li>
								</skills>
							  </li>
							  <li Class="ProjectRimFactory.Common.ModExtension_BonusYield">
								<bonusYields Chance="1">
								</bonusYields>
							  </li>
							</modExtensions>
							<staticSunShadowHeight>1</staticSunShadowHeight>
							<researchPrerequisites>
							  <li>PRF_AnimalStationsIII</li>
							</researchPrerequisites>
						  </ThingDef>
					</value>
				</li>
				<!-- ============== Adding fish food =============== -->
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						  <ThingDef ParentName="OrganicProductBase">
							<defName>PRF_FishFood</defName>
							<label>fish food</label>
							<description>Animal feed made from mixed meat and plant sources, used for feeding fish in tanks.  Humanoids should not eat this.</description>
							<thingCategories>
							  <li>Foods</li>
							</thingCategories>
							<graphicData>
							  <texPath>Resources/PRF_FishFood</texPath>
							</graphicData>
							<socialPropernessMatters>true</socialPropernessMatters>
							<statBases>
							  <MarketValue>1.1</MarketValue>
							  <Mass>0.015</Mass>
							  <Nutrition>0.05</Nutrition>
							  <FoodPoisonChanceFixedHuman>0.2</FoodPoisonChanceFixedHuman>
							</statBases>
							<ingestible>
							  <foodType>Kibble</foodType>
							  <preferability>RawBad</preferability>
							  <tasteThought>AteKibble</tasteThought>
							  <optimalityOffsetHumanlikes>-30</optimalityOffsetHumanlikes>
							  <optimalityOffsetFeedingAnimals>15</optimalityOffsetFeedingAnimals>
							  <ingestEffect>EatVegetarian</ingestEffect>
							  <ingestSound>RawVegetable_Eat</ingestSound>
							</ingestible>
							<comps>
							  <li Class="CompProperties_Ingredients" />
							</comps>
						  </ThingDef>
					</value>
				</li>
				<!-- ============== Adding recipe for fish food =============== -->
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						  <RecipeDef>
							<defName>Make_PRF_FisdFood</defName>
							<label>make fish food</label>
							<description>Make fish food by combining raw meat and plants.</description>
							<jobString>Making fish food.</jobString>
							<workAmount>250</workAmount>
							<workSpeedStat>CookSpeed</workSpeedStat>
							<effectWorking>Cook</effectWorking>
							<soundWorking>Recipe_CookMeal</soundWorking>
							<allowMixingIngredients>true</allowMixingIngredients>
							<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
							<ingredients>
							  <li>
								<filter>
								  <categories>
									<li>MeatRaw</li>
								  </categories>
								</filter>
								<count>1</count>
							  </li>
							  <li>
								<filter>
								  <categories>
									<li>PlantFoodRaw</li>
								  </categories>
								</filter>
								<count>1</count>
							  </li>
							</ingredients>
							<products>
							  <PRF_FishFood>5</PRF_FishFood>
							</products>
							<fixedIngredientFilter>
							  <categories>
								<li>FoodRaw</li>
								<li>PlantFoodRaw</li>
							  </categories>
							</fixedIngredientFilter>
							<workSkill>Cooking</workSkill>
							<recipeUsers>
								<li>ButcherSpot</li>
								<li>TableButcher</li>
							</recipeUsers>
						  </RecipeDef>
					</value>
				</li>
				<!-- ============== Adding freshwater fish recipe =============== -->
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						  <RecipeDef>
							<defName>PRF_FreshwaterFish</defName>
							<label>grow freshwater fish</label>
							<description>Start process of farming freshwater fish.</description>
							<jobString>farming fish.</jobString>
							<effectWorking>Cook</effectWorking>
							<workSkill>Cooking</workSkill>
							<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
							<soundWorking>Recipe_CookMeal</soundWorking>
							<workAmount>5000</workAmount>
							<ingredients>
							  <li>
								<filter>
								  <thingDefs>
									<li>PRF_FishFood</li>
								  </thingDefs>
								</filter>
								<count>10</count>
							  </li>
							</ingredients>
							<products>
							</products>
						  </RecipeDef>
					</value>
				</li>
				<!-- ============== Adding freshwater fish recipe =============== -->
				<li Class="PatchOperationAdd">
					<xpath>/Defs</xpath>
					<value>
						  <RecipeDef>
							<defName>PRF_OceanwaterFish</defName>
							<label>grow oceanwater fish</label>
							<description>Start process of farming oceanwater fish.</description>
							<jobString>farming fish.</jobString>
							<effectWorking>Cook</effectWorking>
							<workSkill>Cooking</workSkill>
							<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
							<soundWorking>Recipe_CookMeal</soundWorking>
							<workAmount>6000</workAmount>
							<ingredients>
							  <li>
								<filter>
								  <thingDefs>
									<li>PRF_FishFood</li>
								  </thingDefs>
								</filter>
								<count>15</count>
							  </li>
							</ingredients>
							<products>
							</products>
						  </RecipeDef>
					</value>
				</li>
			</operations>
		</match>
	</Operation>

</Patch>